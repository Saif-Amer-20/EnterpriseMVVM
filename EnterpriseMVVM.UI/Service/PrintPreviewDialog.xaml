<Window x:Class="EnterpriseMVVM.UI.Service.PrintPreviewDialog"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		                     xmlns:mohsenmou="https://github.com/mohsenmou/ui-wpf"
                             mc:Ignorable="d"
                             Title="Print Preview"
                             ResizeMode="CanResizeWithGrip"
                             Height="450"
                             Width="800"
                             mohsenmou:WindowSettings.Save="True"
                             Style="{DynamicResource MainWindowStyle}">
    <Grid Background="{mohsenmou:ThemeResource ResourceKey=ContentBackground}">
            <mohsenmou:CustomDocumnetViewer x:Name="documentViewer" />
    </Grid>
</Window>
